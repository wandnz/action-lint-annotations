{
    "problemMatcher": [
        {
            "owner": "isort_severity",
            "pattern": [
                {
                    "regexp": "^#{0,2}\\[([a-z]*)\\](\\S*): (\\S*) (.*)$",
                    "file": 3,
                    "severity": 1,
                    "message": 4
                }
            ]
        }
    ]
}
